{
  "private": true,
  "scripts": {
    "clean": "rm -rf cache",
    "build-client": "ts-node client/client.ts",
    "build-data": "ts-node data/data.ts",
    "build-worker": "../../../target/release/edgesearch build --documents data/documents.txt --document-encoding json --document-terms data/terms.txt --maximum-query-results 50 --output-dir ../dist/worker",
    "deploy-worker": "../../../target/release/edgesearch deploy --default-results data/default-results.txt --account-id $CF_ACCOUNT_ID --account-email $CF_ACCOUNT_EMAIL --global-api-key $CF_GLOBAL_API_KEY --name $CF_WORKER_NAME --output-dir ../dist/worker --namespace $CF_KV_NAMESPACE_ID --upload-data",
    "test-worker": "node ../../../tester/server.js --output-dir ../dist/worker --namespace $CF_KV_NAMESPACE_ID --port 8180 --default-results data/default-results.txt"
  },
  "dependencies": {
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3",
    "mkdirp": "^1.0.3",
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-arrow-functions": "^7.0.0",
    "@babel/plugin-transform-block-scoping": "^7.1.5",
    "@babel/plugin-transform-destructuring": "^7.1.3",
    "@babel/plugin-transform-for-of": "^7.0.0",
    "@babel/plugin-transform-parameters": "^7.1.0",
    "@babel/plugin-transform-shorthand-properties": "^7.0.0",
    "@babel/plugin-transform-spread": "^7.0.0",
    "@babel/plugin-transform-template-literals": "^7.0.0",
    "clean-css": "^4.2.1",
    "handlebars": "^4.0.11",
    "hyperbuild": "^0.0.43",
    "ncp": "^2.0.0",
    "terser": "^4.6.3",
    "cheerio": "^1.0.0-rc.2",
    "html-entities": "^1.2.1",
    "moment": "2.24.0",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^13.7.0",
    "@types/babel__core": "^7.1.3",
    "@types/clean-css": "^4.2.1",
    "@types/ncp": "^2.0.3",
    "@types/cheerio": "^0.22.16",
    "@types/html-entities": "^1.2.16",
    "@types/request": "^2.48.5"
